#include <iostream>

using namespace std;

int main()
{
    double nilai;
    char indeks;

    //memberikan informasi agar user memasukkan sebuah biangan bulat
    cout << "Masukkan nilai yang diperoleh :  ";
    cin >> nilai;

    cout << endl << endl;

    if (nilai >=85)
        {indeks = 'A';}
    else if (nilai >=70)
        {indeks = 'B';}
    else if (nilai >=55)
        {indeks = 'C';}
    else if (nilai >=40)
        {indeks = 'D';}
    else
        {indeks = 'E';}
    cout << "Nilai indeks dari nilai " << nilai << " " << "adalah " << indeks << endl;

return 0;
}
